<!doctype html>
<html lang="ru">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="description" content="">
        <title>
            {% block title %}{% endblock %} | {% block sitename %}{% endblock %}
        </title>
        <link href="{{ asset('build/app.css') }}" rel="stylesheet">
        <style>
            @font-face {
                font-family: 'Ubuntu';
                font-style: normal;
                font-weight: 400;

                src:
                    url({{ asset('build/fonts/ubuntu-all-400-normal.woff') }});
            }
        </style>
        <link href="{{ asset('build/main/main.css') }}" rel="stylesheet">
        <link href="{{ asset('build/main/header.css') }}" rel="stylesheet">
        {% block styles %}{% endblock %}
    </head>
    <body>
        <svg xmlns="http://www.w3.org/2000/svg" style="display: none;">
            <symbol id="people-circle" viewBox="0 0 16 16">
                <path d="M11 6a3 3 0 1 1-6 0 3 3 0 0 1 6 0z"/>
                <path fill-rule="evenodd" d="M0 8a8 8 0 1 1 16 0A8 8 0 0 1 0 8zm8-7a7 7 0 0 0-5.468 11.37C3.242 11.226 4.805 10 8 10s4.757 1.225 5.468 2.37A7 7 0 0 0 8 1z"/>
            </symbol>
        </svg>
        {{ include('genesis/main/header.html.twig') }}
        {% block content %}{% endblock %}
    </body>
    <script src="{{ asset('build/runtime.js') }}"></script>
    <script src="{{ asset('build/app.js') }}"></script>
    <script src="{{ asset('build/main/main.js') }}"></script>
    <script src="{{ asset('build/main/header.js') }}"></script>
    {% block scripts %}{% endblock %}
</html>